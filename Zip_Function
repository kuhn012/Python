my_list = [1,2,3]
your_list= [10,20,20]
their_list = [4,5,3]

def multiply_by2(item):
    return item * 2

def check_if_odd(item):
    return item % 2 != 0

print(list(zip(my_list, your_list, their_list)))
print(my_list)

##Zip Function only uses iterables as inputs
##Returns an object, that has tupples of data for each index
